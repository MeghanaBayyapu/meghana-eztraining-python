   "source": [
    "import time\n",
    "def countdown(t):\n",
    "    while t:\n",
    "        mins,secs = divmod(t,60)\n",
    "        timer = '{:02d}:{:02d}'.format(mins,secs)\n",
    "        print(timer)\n",
    "        time.sleep(1)\n",
    "        t = t-1 \n",
    "    print(\"Finished\")\n",
    "t = int(input(\"Enter the time in seconds:\" ))\n",
    "countdown(t)"
   ]
